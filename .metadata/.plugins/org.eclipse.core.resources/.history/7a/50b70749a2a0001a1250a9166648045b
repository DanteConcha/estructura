package arbol;
import javax.swing.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Arbol arbol = new Arbol();
		int totalpalabras=Integer.parseInt(JOptionPane.showInputDialog(null,"total de numeros"));

		String n;
		for(int i=0;i<totalpalabras;i++){
			n=JOptionPane.showInputDialog(null,"palabra: ");
			arbol.creaArbolBinario(n);
		}
		arbol.entreOrden(arbol.getRaiz());
	}

}
